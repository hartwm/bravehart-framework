{
  "browserSyncOptions": {
    "notify": false,
    "server": {
      "baseDir": "./public"
    }
  },
  "browserSyncWatchFiles": [
    "./css/*.css",
    "./js/*.js",
    "./**/*.php",
    "./**/*.html"
  ],
  "paths": {
    "js": "./js",
    "css": "./css",
    "img": "./img",
    "scss": "./scss",
    "node": "./node_modules/",
    "dev": "./app",
    "public": "./public",
    "dist": "./dist",
    "vendor": ""
  }
}
